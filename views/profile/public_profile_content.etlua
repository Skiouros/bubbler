                        <li style="width:55px;">
                            <img class="img-circle" src="/static/img/avatars/avatar6.jpg" alt="" width="45px" height="45px">
                        </li>
                        <li class="text-left">
                            <h5 class="semibold ellipsis">
                                <%= user.name %><br>
                                <small class="text-muted">@johndoe</small>
                            </h5>
                        </li>
                        <li class="text-left">
                            <button class="btn btn-sm btn-info" data-toggle="class"><i class="ico-user-plus2"></i> Follow Me</button>
                        </li>
                    </ul>
                </div>
                <ul class="nav nav-justified mt15">
                    <li class="text-center">
                        <h4 class="nm">100</h4>
