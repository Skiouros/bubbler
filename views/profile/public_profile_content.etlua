                    </div>
                    <hr class="mt0 mb15"/>
                    <div class="row">
                        <div class="masonry">
                            <% if #books > 0 then %>
                                <% for i, book in ipairs(books) do %>
                                    <% render("views.templates.book", book) %>
                                <% end %>
